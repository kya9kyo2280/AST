<div class="container">
      <div class="row">
        <div class="col-4">
        </div>

<div class = "contact">
    <h1>お問合せ</h1>
    <div class = "contact-body">
<%= form_for(@contact) do |f| %>

    <h4>タイトル</h4>
    <%= f.text_field :contact_title %>

    <h4>本文</h4>
    <%= f.text_area :contact_message %>

    <%= f.submit '投稿' %>

<% end %>
</div>
</div>
</div>
</div>
</div>
